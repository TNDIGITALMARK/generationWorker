{
  "watch": [
    "index.ts",
    "firebase/**/*.ts",
    "package.json",
    "tsconfig.json",
    ".env"
  ],
  "ignore": [
    "node_modules/**/*",
    "dist/**/*",
    "python_service/**/*",
    "ComfyUI/**/*",
    "*.log",
    "*.tmp",
    "*.temp",
    ".git/**/*",
    "**/*.pyc",
    "**/__pycache__/**/*"
  ],
  "ext": "ts,js,json",
  "exec": "ts-node index.ts",
  "env": {
    "NODE_ENV": "development"
  }
}